    "#removing list of integers\n",
    "evenlist=[10,27,37,44,51,53,82]\n",
    "print(\"list items =\",evenlist)\n",
    "for ev in evenlist:\n",
    "    if(ev % 2 ==0):\n",
    "        evenlist.remove(ev)\n",
    "print(\"list items after removing items = \",evenlist)"
