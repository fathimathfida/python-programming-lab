   "source": [
    "#dictionery sort in ascending and descending order\n",
    "a={'orange':12,'apple':13,'mango':15,'lemon':14}\n",
    "l=list(a.items())\n",
    "l.sort()\n",
    "print('Ascending order is:',l)\n",
    "l=list(a.items())\n",
    "l.sort(reverse=True)\n",
    "print('Descending order is:',l)\n",
    "dict=dict(l)\n",
    "print(\"dictionery\",dict)"
   ]
